			$('.link-login').on('click', function() {
				$('#modalLogin .modal-body-title-login').addClass("selected");
				$('#modalLogin .modal-body-title-signup').addClass("unselected");
				$('#modalLogin').modal();
			});
			$('#modalLogin .modal-body-title-signup').on('click', function() {
				$('#modalLogin').modal('hide'); 
				$('.link-signup').click();
			});
		});
	</script>
</body>
</html>